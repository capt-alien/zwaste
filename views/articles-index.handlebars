<!-- reviews-index -->
<h1>Articles</h1>


<a href="/articles/new">New Article</a>

<div class="row">
{{#each articles}}

 <div class="col-md-3">
<h2><a href="/articles/{{this._id}}">{{this.name}}</a></h2>

 <h3>
  <font size="3" color="red">{{this.bin}}</font>
</h3>
  <small>{{this.description}}</small>
</div>

{{/each}}

</div>
